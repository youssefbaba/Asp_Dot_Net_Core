@model PersonGridModel

<div class="box">
    @*<h3>@ViewBag.Grid.GridTitle</h3>*@
    <h3>@Model.GridTitle</h3>
    <table class="table w-100">
        <thead>
            <tr>
                <th>Name</th>
                <th>Job</th>
            </tr>
        </thead>
        <tbody>
            @*@foreach (Person person in ViewBag.Grid.People)
            {
                <tr>
                    <td>@person.PersonName</td>
                    <td>@person.JobTitle</td>
                </tr>
            }*@

            @foreach (Person person in Model.People)
            {
                <tr>
                    <td>@person.PersonName</td>
                    <td>@person.JobTitle</td>
                </tr>
            }
        </tbody>
    </table>
</div>
